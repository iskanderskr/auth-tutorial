{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "rules": {
		"prefer-const": "error",
		"no-use-before-define": "error",
		"no-whitespace-before-property": "error",
		"array-bracket-spacing": "error",
		"no-unused-vars": "error",
		"no-console": [
			"warn",
			{
				"allow": ["warn", "error"]
			}
		],
		"prettier/prettier": [
			"error"
		]
	}
}
